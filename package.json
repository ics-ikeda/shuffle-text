{
  "name": "shuffle-text",
  "version": "0.2.1",
  "repository": "ics-ikeda/shuffle-text",
  "devDependencies": {
    "lite-server": "^2.3.0",
    "rollup": "^0.41.6",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-watch": "^3.2.2",
    "typescript": "^2.3.2",
    "uglify-js": "^2.8.22"
  },
  "main": "build/shuffle-text.js",
  "jsnext:main": "build/shuffle-text.module.js",
